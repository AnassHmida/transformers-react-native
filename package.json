{
  "name": "@aneshmida/transformers-react-native",
  "version": "0.1.0",
  "description": "Run ðŸ¤— Transformers directly in React Native",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "source": "src/index",
  "type": "module",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "types",
    "transformers-react-native.podspec",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "release": "release-it",
    "example": "yarn --cwd example",
    "bootstrap": "yarn example && yarn install",
    "typegen": "tsc ./src/index.ts --declaration --emitDeclarationOnly --declarationMap --outDir types",
    "build": "webpack && yarn typegen",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose"
  },
  "keywords": [
    "react-native",
    "transformers",
    "transformers.js",
    "machine-learning",
    "huggingface",
    "artificial intelligence",
    "AI",
    "ML"
  ],
  "author": "Anes Hmida",
  "license": "MIT",
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "react-native-fs": "^2.20.0",
    "react-native-buffer": "^6.0.3",
    "onnxruntime-react-native": "^1.17.0",
    "browserify-zlib": "^0.2.0",
    "path-browserify": "^1.0.1",
    "@huggingface/jinja": "^0.2.2",
    "image-codecs": "^0.1.0",
    "node-wav": "^0.0.2",
    "onnxruntime-web": "^1.17.0",
    "sharp": "^0.32.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.0.2",
    "@types/react": "~17.0.21",
    "@types/react-native": "0.70.0",
    "@types/jest": "^29.5.1",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-builder-bob": "^0.20.0",
    "catharsis": "github:xenova/catharsis",
    "copy-webpack-plugin": "^11.0.0",
    "jest": "^29.5.0",
    "jest-environment-node": "^29.5.0",
    "jsdoc-to-markdown": "^8.0.1",
    "typescript": "^5.2.2",
    "wavefile": "^11.0.0",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.13.3"
  },
  "react-native": {
    "fs": "react-native-fs",
    "onnxruntime-web": false,
    "onnxruntime-node": "onnxruntime-react-native",
    "path": "path-browserify",
    "stream/web": false,
    "zlib": "browserify-zlib"
  },
  "overrides": {
    "jpeg-js": "^0.4.4",
    "semver": "^7.5.4",
    "protobufjs": "^7.2.6"
  },
  "publishConfig": {
    "access": "public"
  },
   "jsdelivr": "./dist/transformers.min.js",
  "unpkg": "./dist/transformers.min.js"
}
